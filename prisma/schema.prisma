generator client {
  provider = "prisma-client"
  output   = "../prisma/build"
}

datasource db {
  provider = "mysql"
}

model Users {
  id         String   @id @default(cuid())
  name       String?
  email      String   @unique
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
